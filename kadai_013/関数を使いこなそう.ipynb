      "source": [
        "# 消費税を加えた金額を計算する関数\n",
        "def calculate_total(price, tax):\n",
        "    # tax は「％」なので、100で割って計算する\n",
        "    total = price + (price * tax / 100)\n",
        "    return total\n",
        "\n",
        "# 関数を呼び出す（第1引数：金額110、第2引数：消費税10%）\n",
        "result = calculate_total(110, 10)\n",
        "\n",
        "# 結果を表示\n",
        "print(result)\n"
      ]
